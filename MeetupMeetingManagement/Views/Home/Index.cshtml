<div ng-controller="MemberListCtrl as ctrl">
	<div class="row">
		<div class="col-md-4">
			<table>
				<tr>
					<th>First Name</th>
					<th>Last Name</th>
				</tr>
				<tr ng-repeat="member in ctrl.memberList">
					<td>{{member.fName}}</td>
					<td>{{member.lName}}</td>
				</tr>
			</table>
		</div>
	</div>
</div>
